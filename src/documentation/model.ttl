PREFIX activiteit:           <https://data.omv.vlaanderen.be/id/activiteit/>
PREFIX adms:                 <http://www.w3.org/ns/adms#>
PREFIX concept:              <https://data.omv.vlaanderen.be/id/concept/>
PREFIX cs:                   <https://data.vlaanderen.be/ns/chemische_stof#>
PREFIX csc:                  <https://data.omgeving.vlaanderen.be/id/concept/chemische_stof/>
PREFIX dbo:                  <http://dbpedia.org/ontology/>
PREFIX dbp:                  <http://dbpedia.org/property/>
PREFIX dc:                   <http://purl.org/dc/elements/1.1/>
PREFIX dct:                  <http://purl.org/dc/terms/>
PREFIX eurovoc:              <http://eurovoc.europa.eu/>
PREFIX geo:                  <http://www.opengis.net/ont/geosparql#>
PREFIX geometry:             <https://data.omv.vlaanderen.be/id/geometry/>
PREFIX ident:                <https://data.omv.vlaanderen.be/id/identificator/>
PREFIX inh:                  <https://data.omv.vlaanderen.be/id/inhoud/>
PREFIX juridische_categorie: <https://data.omv.vlaanderen.be/id/concept/juridische_categorie/>
PREFIX omv:                  <https://data.vlaanderen.be/ns/omgevingsvergunning#>
PREFIX omv_id:               <https://data.omv.vlaanderen.be/id/concept/identifier/>
PREFIX owl:                  <http://www.w3.org/2002/07/owl#>
PREFIX perceel:              <https://data.vlaanderen.be/ns/perceel#>
PREFIX project:              <https://data.omv.vlaanderen.be/id/project/>
PREFIX project_type_code:    <https://data.omv.vlaanderen.be/id/concept/procedure/project_type_code/>
PREFIX rdf:                  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:                 <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ruimtelijke_eenheid:  <https://data.omv.vlaanderen.be/id/ruimtelijke_eenheid/>
PREFIX skos:                 <http://www.w3.org/2004/02/skos/core#>
PREFIX vlcs:                 <https://data.omgeving.vlaanderen.be/id/conceptscheme/>
PREFIX voorwerp:             <https://data.omv.vlaanderen.be/id/voorwerp/>
PREFIX xsd:                  <http://www.w3.org/2001/XMLSchema#>
PREFIX zaak:                 <https://data.omv.vlaanderen.be/id/zaak/>
PREFIX zaak_handeling:       <https://data.omv.vlaanderen.be/id/zaakhandeling/>

dbo:Case  rdf:type         "rdfs:Resource";
        adms:identifier    adms:Identifier;
        omv:procedure      omv:Procedure;
        omv:verwanteZaak   dbo:Case;
        omv:zaakhandeling  omv:Rechtshandeling , omv:Zaakhandeling .

omv:Inhoud  rdf:type      "rdfs:Resource";
        <http://uit.te.klaren#modaliteit>
                "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString";
        omv:rechtsobject  omv:Activiteit .

omv:NormatieveBepaling
        rdf:type          "rdfs:Resource";
        <http://uit.te.klaren#modaliteit>
                "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString";
        omv:rechtsobject  omv:Activiteit .

omv:Procedure  rdf:type  "rdfs:Resource" .

adms:Identifier  rdf:type  "rdfs:Resource";
        rdf:value      "http://www.w3.org/2001/XMLSchema#integer";
        #rdfs:label     "http://www.w3.org/2001/XMLSchema#integer";
        dc:source      "http://www.w3.org/2001/XMLSchema#string";
        dct:type       "rdfs:Resource";
        skos:notation  "https://data.omv.vlaanderen.be/id/concept/identifier/omgevingsloket_record_id" , "https://data.omv.vlaanderen.be/id/concept/identifier/omv_nummer" , "https://data.omv.vlaanderen.be/id/concept/identifier/voorwerp_id" .

skos:Concept  rdf:type  "rdfs:Resource" .

geo:Geometry  rdf:type  "rdfs:Resource";
        geo:asWKT  "http://www.opengis.net/ont/geosparql#wktLiteral" .

omv:Rechtshandeling  rdf:type  "rdfs:Resource";
        omv:ingangsdatum  "http://www.w3.org/2001/XMLSchema#date";
        omv:inhoud        omv:Inhoud , omv:NormatieveBepaling .

perceel:RuimtelijkeEenheid
        rdf:type             "rdfs:Resource";
        geo:hasGeometry      geo:Geometry;
        adms:identifier      adms:Identifier;
        perceel:RuimtelijkeEenheid.oppervlakte
                "http://www.w3.org/2001/XMLSchema#integer";
        perceel:landgebruik  skos:Concept .

omv:Activiteit  rdf:type            "rdfs:Resource";
        omv:juridischeKwalificatie  "rdfs:Resource";
        omv:locatie                 perceel:RuimtelijkeEenheid .

omv:Zaakhandeling  rdf:type  "rdfs:Resource";
        omv:ingangsdatum  "http://www.w3.org/2001/XMLSchema#date";
        omv:inhoud        omv:Inhoud , omv:NormatieveBepaling .
